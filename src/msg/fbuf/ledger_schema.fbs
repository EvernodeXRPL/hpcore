include "common_schema.fbs";

namespace msg.fbuf.ledger;

table LedgerBlock {
    seq_no:uint64;
    time:uint64;
    lcl:[ubyte];
    state:[ubyte];
    users: [ByteArray];
    inputs: [ByteArray]; 
    outputs: [ByteArray];
}

table FullHistoryBlock {
    raw_inputs: [RawInput];
}

table RawInput {
    hash:[ubyte];
    pubkey:[ubyte];
    input_container:[ubyte];
    sig:[ubyte];
    protocol:uint8;
}

root_type LedgerBlock;